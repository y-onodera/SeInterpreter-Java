<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane id="paneSeInterpreterMenu" fx:id="paneSeInterpreterMenu" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.sebuilder.interpreter.javafx.view.menu.MenuPresenter">
    <children>
        <MenuBar id="menuBar" fx:id="menuBar" prefWidth="Infinity"
                 AnchorPane.bottomAnchor="0.0"
                 AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                 AnchorPane.topAnchor="0.0">
            <menus>
                <Menu id="menuScript" fx:id="menuScript" mnemonicParsing="false" text="Script">
                    <items>
                        <MenuItem id="menuTestOpenFile" fx:id="menuScriptOpenFile" mnemonicParsing="false"
                                  onAction="#handleScriptOpenFile" text="Open File"/>
                        <MenuItem id="menuTestImportSeleniumIDEScript" fx:id="menuImportSeleniumIDEScript" mnemonicParsing="false"
                                  onAction="#handleImportSeleniumIDEScript" text="Import SeleniumIDE Script"/>
                        <MenuItem id="menuTestSuiteSave" fx:id="menuSaveSuite" mnemonicParsing="false"
                                  onAction="#handleSaveSuite" text="Save Suite"/>
                        <MenuItem id="menuTestSuiteSaveAs" fx:id="menuSaveSuiteAs" mnemonicParsing="false"
                                  onAction="#handleSaveSuiteAs" text="Save Suite As"/>
                        <MenuItem id="menuTestNew" fx:id="menuScriptNew" mnemonicParsing="false"
                                  onAction="#handleCreateNewSuite" text="Create New Suite"/>
                    </items>
                </Menu>
                <Menu id="menuReplay" fx:id="menuReplay" mnemonicParsing="false" text="Replay">
                    <items>
                        <MenuItem id="menuRunSuite" fx:id="menuBrowser0RunSuite" mnemonicParsing="false"
                                  onAction="#handleReplaySuite" text="Run Suite"/>
                        <MenuItem id="menuRunScript" fx:id="menuBrowserRunScript" mnemonicParsing="false"
                                  onAction="#handleReplayScript" text="Run Script"/>
                        <MenuItem fx:id="menuReplaySetting" mnemonicParsing="false" onAction="#handleReplaySetting"
                                  text="Setting"/>
                        <MenuItem fx:id="menuOpenResult" mnemonicParsing="false" onAction="#handleOpenResult"
                                  text="Open Result"/>
                    </items>
                </Menu>
                <Menu id="menuBrowser" fx:id="menuBrowser" mnemonicParsing="false" text="Browser">
                    <items>
                        <MenuItem fx:id="menuBrowserOpen" mnemonicParsing="false" onAction="#handleBrowserOpen"
                                  text="Open"/>
                        <MenuItem fx:id="menuBrowserClose" mnemonicParsing="false" onAction="#handleBrowserClose"
                                  text="Close"/>
                        <MenuItem fx:id="menuBrowserSetting" mnemonicParsing="false" onAction="#handleBrowserSetting"
                                  text="Setting"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </children>
</AnchorPane>
