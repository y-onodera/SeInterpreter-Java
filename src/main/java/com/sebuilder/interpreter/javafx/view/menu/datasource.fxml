<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.sebuilder.interpreter.javafx.view.menu.DatasourcePresenter">
    <SplitPane dividerPositions="0.5" orientation="VERTICAL" maxHeight="80.0" prefHeight="80.0" prefWidth="400.0">
        <AnchorPane maxHeight="50.0" prefHeight="50.0">
            <GridPane maxHeight="30.0" prefHeight="30.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="90.0" prefWidth="90.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="250.0" prefWidth="250.0"/>
                    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="60.0"
                                       prefWidth="60.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="15.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <Label text="datasource dir" GridPane.rowIndex="2"/>
                <TextField fx:id="datasourceText" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Button fx:id="datasourceSearchButton" onAction="#dataSourceSearch"
                        mnemonicParsing="false" text="search" GridPane.columnIndex="2"
                        GridPane.rowIndex="2"/>
            </GridPane>
        </AnchorPane>
        <AnchorPane maxHeight="30.0" prefHeight="30.0" prefWidth="400.0">
            <GridPane>
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="350.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="50.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" valignment="CENTER"/>
                </rowConstraints>
                <Button fx:id="editButton" onAction="#settingEdit" mnemonicParsing="false" text="Edit"
                        GridPane.columnIndex="1" GridPane.halignment="CENTER"
                        GridPane.valignment="CENTER"/>
            </GridPane>
        </AnchorPane>
    </SplitPane>
</AnchorPane>
