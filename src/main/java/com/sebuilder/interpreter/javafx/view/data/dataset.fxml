<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.*?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.sebuilder.interpreter.javafx.view.data.DataSetPresenter">
    <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="220.0" prefWidth="400.0"
               AnchorPane.bottomAnchor="0.0"
               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
        <AnchorPane fx:id="gridParentPane"/>
        <AnchorPane prefHeight="30.0" minHeight="30.0" maxHeight="30.0" prefWidth="350.0">
            <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0">
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveDataSet"
                        text="save" GridPane.columnIndex="1" GridPane.halignment="CENTER"
                        GridPane.valignment="CENTER"/>
                <Button fx:id="reloadButton" mnemonicParsing="false" onAction="#reloadDataSet"
                        text="reload" GridPane.columnIndex="2" GridPane.halignment="CENTER"
                        GridPane.valignment="CENTER"/>
            </GridPane>
        </AnchorPane>
    </SplitPane>
</AnchorPane>
