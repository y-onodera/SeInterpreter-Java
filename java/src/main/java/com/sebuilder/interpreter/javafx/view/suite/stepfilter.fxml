<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import org.tbee.javafx.scene.layout.fxml.MigPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.sebuilder.interpreter.javafx.view.suite.StepFilterPresenter"
            prefHeight="400.0" prefWidth="420.0">
    <SplitPane orientation="VERTICAL"
               prefWidth="420.0"
               maxHeight="Infinity" minHeight="0">
        <fx:include fx:id="filterTable" source="../filter/filterTable.fxml"/>
        <MigPane cols="[grow,right][fill,right]" rows="[]" maxHeight="40" prefWidth="420.0">
            <Button mnemonicParsing="false" text="Commit" onAction="#commit" MigPane.cc="align right"/>
            <Button mnemonicParsing="false" text="Save As" onAction="#saveAs" MigPane.cc="align right"/>
        </MigPane>
    </SplitPane>
</AnchorPane>
