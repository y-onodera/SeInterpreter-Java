{
    "data": {
        "configs": {"csv": {"path": "formInput.csv"}},
        "source": "csv"
    },
    "steps": [
        {
            "locator": {
                "type": "id",
                "value": "reserve_year"
            },
            "negated": "false",
            "skip": "false",
            "type": "storeElementValue",
            "variable": "reserve_year"
        },
        {
            "locator": {
                "type": "id",
                "value": "reserve_month"
            },
            "negated": "false",
            "skip": "false",
            "type": "storeElementValue",
            "variable": "reserve_month"
        },
        {
            "locator": {
                "type": "id",
                "value": "reserve_day"
            },
            "negated": "false",
            "skip": "false",
            "type": "storeElementValue",
            "variable": "reserve_day"
        },
        {
            "negated": "false",
            "script": "return ${reserve_year} + '/' + ${reserve_month} + '/' + (${reserve_day} + ${add_days});",
            "skip": "false",
            "type": "storeEval",
            "variable": "date"
        },
        {
            "locator": {
                "type": "id",
                "value": "datePick"
            },
            "negated": "false",
            "skip": "false",
            "text": "${date}",
            "type": "SetElementText"
        },
        {
            "negated": "false",
            "build.xml": "${_dataSourceDir}/build.xml",
            "target": "nextDate",
            "type": "storeAntRun",
            "variable": "date",
            "resultProperty": "resultDate"
        },
        {
            "locator": {
                "type": "id",
                "value": "datePick"
            },
            "negated": "false",
            "skip": "false",
            "text": "${date}",
            "type": "SetElementText"
        },
        {
            "locator": {
                "type": "id",
                "value": "breakfast_on"
            },
            "negated": "false",
            "skip": "${id:breakfast_on}",
            "type": "SetElementSelected"
        },
        {
            "locator": {
                "type": "id",
                "value": "breakfast_off"
            },
            "negated": "false",
            "skip": "${id:breakfast_off}",
            "type": "SetElementSelected"
        },
        {
            "type": "ifElementSelected",
            "locator": {
                "type": "id",
                "value": "breakfast_on"
            },
            "subStep": "1"
        },
        {
            "file": "if_test.png",
            "scroll": "false",
            "type": "SaveScreenshot"
        },
        {
            "type": "Loop",
            "subStep": "2",
            "count": "3"
        },
        {
            "check": "${get('_index') % 2 == 0 ? get('id:plan_a') == 'false' : get('id:plan_a') == 'true' }",
            "locator": {
                "type": "id",
                "value": "plan_a"
            },
            "negated": "false",
            "skip": "false",
            "type": "SetElementSelected"
        },
        {
            "check": "${get('_index') % 2 == 0 ? get('id:plan_b') == 'false' : get('id:plan_b') == 'true' }",
            "locator": {
                "type": "id",
                "value": "plan_b"
            },
            "negated": "false",
            "skip": "false",
            "type": "SetElementSelected"
        },
        {
            "locator": {
                "type": "id",
                "value": "guestname"
            },
            "negated": "false",
            "skip": "${!has('id:guestname')}",
            "text": "${id:guestname}",
            "type": "SetElementText"
        },
        {
            "check": "true",
            "locator": {
                "type": "id",
                "value": "reserve_term"
            },
            "value": "${id:reserve_term}",
            "skip": "false",
            "type": "SelectElementValue"
        },
        {
            "check": "true",
            "locator": {
                "type": "id",
                "value": "headcount"
            },
            "value": "${id:headcount}",
            "skip": "false",
            "type": "SelectElementValue"
        }
    ]
}